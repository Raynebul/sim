    "startP": "concurrently \"npm run backend\" \"npm run preview\"",
    "startPU": "concurrently \"npm run backend -- --host\" \"npm run preview -- --host\"",
    "startPUE": "concurrently \"npm runbackend -- --host\" \"npm run preview -- --host\"",
    "preview": "vite preview",
    "build": "vite build",
    "script": "node ./src/databases/script.js",
    "build-sim": "vite build --base=/simulator",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
